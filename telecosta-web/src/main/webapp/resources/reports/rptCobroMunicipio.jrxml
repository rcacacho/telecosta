	<queryString>
		<![CDATA[SELECT c.codigo, c.nombres, c.direccion, concat(p.mes, '-',p.anio) fechapago, c.observacion 
FROM pago p 
join cliente c on p.idcliente = c.idcliente and c.activo = 1 
where p.activo =1 
and p.fechapago = (SELECT max(pp.fechapago) from pago pp where pp.idpago = p.idpago GROUP by pp.fechapago) 
and c.idmunicipio = $P{ID_MUNICIPIO}
order by fechapago asc]]>
	</queryString>
